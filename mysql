docker pull mysql:5.7
docker run -p 3306:3306 --name mysql57 --network projectdw2019_mongo_net -v /home/mistert/Escritorio/Project-Dw2019/keycloak/docker_images/mysql:/var/lib/mysql -e MYSQL_ROOT_PASSWORD=secret -d  mysql:5.7 --character-set-server=utf8 --collation-server=utf8_unicode_ci
docker exec -it mysql57 mysql -uroot -p

para conectarse romoto:

GRANT ALL PRIVILEGES ON *.* TO 'root'@'%' IDENTIFIED BY 'secret' WITH GRANT OPTION


